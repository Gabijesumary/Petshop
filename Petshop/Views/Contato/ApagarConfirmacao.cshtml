@{
    ViewData["Title"] = "Confirmação de apagar o contato";
}


<div class="text-center">
    <h3>Realmente deseja apagar o contato XXXXX?</h3>
    <br />
    <a class="btn btn-primary" role="button" asp-controller="Contato" asp-action="Index">Cancelar excluão</a>
    <a class="btn btn-danger" role="button" asp-controller="Contato" asp-action="Apagar">Confirmar excluão</a>
</div>
